{
  "author": "Maciej Treder <contact@maciejtreder.com>",
  "description": "Angular Universal PWA boilerplate for serverless environment.",
  "name": "angular-universal-pwa",
  "version": "2.0.0-beta.4",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maciejtreder/angular-universal-serverless.git"
  },
  "scripts": {
    "postinstall": "node postinstall.js",
    "ng": "ng",
    "start": "run-p build:watch credentials",
    "build:watch": "ng serve",
    "credentials": "node credentials.js && opencollective postinstall",
    "test": "ng test --single-run --code-coverage",
    "test:watch": "ng test --code-coverage",
    "build:dev": "run-p test:watch start",
    "build:dev:firebug": "run-p test:watch build:firebug",
    "build:firebug": "npm start --environment=firebug",
    "build": "ng build",
    "lint": "ng lint --fix",
    "build:client-and-server-bundles": "ng build --prod && ng run __projectName__:server",
    "build:prod": "npm run build:client-and-server-bundles && npm run webpack:server",
    "build:deploy": "npm run build:prod && npm run deploy",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "server": "node local.js",
    "deploy": "serverless deploy && npm run credentials"
  },
  "pre-commit": [
    "lint"
  ],
  "keywords": [
    "schematics",
    "angular",
    "serverless",
    "server side rendering",
    "pwa",
    "progressive web app",
    "lambda",
    "amazon web services",
    "google cloud functions"
  ],
  "dependencies": {
    "@angular/animations": "6.0.0-rc.5",
    "@angular/cdk": "6.0.0-rc.11",
    "@angular/common": "6.0.0-rc.5",
    "@angular/compiler": "6.0.0-rc.5",
    "@angular/core": "6.0.0-rc.5",
    "@angular/forms": "6.0.0-rc.5",
    "@angular/http": "6.0.0-rc.5",
    "@angular/material": "6.0.0-rc.11",
    "@angular/platform-browser": "6.0.0-rc.5",
    "@angular/platform-browser-dynamic": "6.0.0-rc.5",
    "@angular/platform-server": "6.0.0-rc.5",
    "@angular/router": "6.0.0-rc.5",
    "@angular/service-worker": "6.0.0-rc.5",
    "@nguniversal/common": "^5.0.0",
    "@nguniversal/express-engine": "^5.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0",
    "@ngx-translate/core": "^10.0.1",
    "@ngx-translate/http-loader": "^3.0.1",
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0-rc.0",
    "typescript-collections": "^1.3.2",
    "zone.js": "^0.8.24",
    "cors": "^2.8.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.5.7",
    "@angular/cli": "6.0.0-rc.5",
    "@angular/compiler-cli": "6.0.0-rc.5",
    "@angular/language-service": "6.0.0-rc.5",
    "@types/jasmine": "^2.8.6",
    "@types/node": "^8.10.7",
    "codelyzer": "^4.0.1",
    "cp-cli": "^1.1.0",
    "cpy-cli": "^1.0.1",
    "express": "^4.15.2",
    "http-server": "^0.10.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "npm-run-all": "^4.1.2",
    "opencollective": "^1.0.3",
    "pre-commit": "^1.2.2",
    "protractor": "~5.1.2",
    "reflect-metadata": "^0.1.10",
    "serverless": "^1.26.1",
    "sinon": "^4.5.0",
    "ts-loader": "^4.2.0",
    "tslint": "^5.7.0",
    "typescript": "~2.7.2",
    "webpack-cli": "^2.0.14"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/angular-universal-pwa"
  }
}
