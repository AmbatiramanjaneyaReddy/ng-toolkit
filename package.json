{
  "author": "Maciej Treder <contact@maciejtreder.com>",
  "description": "Angular Universal boilerplate for serverless environment. Built on top of https://github.com/FrozenPandaz/ng-universal-demo",
  "name": "angular-universal-serverless",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maciejtreder/angular-universal-serverless.git"
  },
  "scripts": {
    "lint": "tslint \"src/**/*.ts\"",
    "test": "npm run lint & karma start",
    "test:dev": "concurrently \"npm run watch\" \"karma start karma.conf.js",
    "pretest": "npm run build",
    "start": "npm run build && npm run server",
    "build": "webpack",
    "build:aot": "webpack --env.aot --env.client & webpack --env.aot --env.server",
    "build:prod:client": "webpack --env.aot --env.client -p",
    "build:prod:server": "webpack --env.aot --env.server",
    "build:prod": "npm run build:prod:client && npm run service-worker && npm run build:prod:server && node src/service-worker/rollup.js",
    "service-worker": "ngu-sw-manifest --module src/app/app.module.ts --lazy-root src/app --out dist/ngsw-manifest.json",
    "build:deploy": "npm run build:prod && npm run deploy",
    "prebuild": "npm run clean",
    "prebuild:aot": "npm run clean",
    "prebuild:prod": "npm run clean",
    "clean": "rimraf dist",
    "server": "nodemon local.js",
    "watch": "concurrently \"webpack --watch\" \"npm run server\"",
    "deploy": "serverless deploy"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/app-shell": "^0.1.0",
    "@angular/cdk": "^2.0.0-beta.8",
    "@angular/common": "4.2.3",
    "@angular/compiler": "4.2.3",
    "@angular/core": "4.2.3",
    "@angular/forms": "4.2.3",
    "@angular/http": "4.2.3",
    "@angular/material": "2.0.0-beta.8",
    "@angular/platform-browser": "4.2.3",
    "@angular/platform-browser-dynamic": "4.2.3",
    "@angular/platform-server": "4.2.3",
    "@angular/router": "4.2.3",
    "@angular/service-worker": "^1.0.0-beta.16",
    "@nguniversal/express-engine": "^1.0.0-beta.2",
    "aws-serverless-express": "3.0.0",
    "body-parser": "^1.17.2",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "express": "^4.15.2",
    "idb": "^2.0.3",
    "ng-http-sw-proxy": "^2.1.1",
    "ng2-device-detector": "^1.0.0",
    "rxjs": "^5.3.0",
    "serverless": "^1.10.0",
    "underscore": "^1.8.3",
    "xhr2": "^0.1.4",
    "zone.js": "^0.8.8"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^4.0.0",
    "@ngtools/webpack": "^1.2.14",
    "@types/express": "^4.0.35",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "^2.5.54",
    "@types/node": "^7.0.8",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.2.3",
    "codelyzer": "^3.1.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.28.0",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine": "^2.7.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "ng-pwa-tools": "0.0.15",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "optimize-js-plugin": "0.0.4",
    "protractor": "^5.1.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "rollup": "^0.43.0",
    "rollup-plugin-commonjs": "7.0.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "sass-loader": "^6.0.5",
    "script-ext-html-webpack-plugin": "^1.7.1",
    "serverless": "^1.15.1",
    "serverless-apigw-binary": "^0.3.22",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "tslint": "^5.6.0",
