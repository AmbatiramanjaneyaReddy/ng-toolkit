{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "es5",
    "noImplicitAny": false,
    "sourceMap": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "rootDir": "./src",
    "baseUrl": "src",
    "lib": [
      "es6",
      "dom"
    ],
    "types": ["node", "hammerjs", "jasmine"]
  },
//  "extends": "./tsconfig.json",
  "angularCompilerOptions": {
    "entryModule": "./src/app/server-app.module#ServerAppModule"
  },
  "files": [
    "./node_modules/@angular-universal-serverless/external-module/release/src/externalComponent/external.module.d.ts"
  ],
  "include": [
    "./src/main.server.ts",
    "./src/app/httpProxy/http-proxy.module.ts",
    "./src/app/lazy/lazy.module.ts"
  ]
}
